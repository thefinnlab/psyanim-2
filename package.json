    "editor": "rimraf -v ./dist && webpack --config ./webpack.editor.config.js && npx nodemon ./src/editor/main.mjs",
    "dev-editor": "rimraf -v ./dist && webpack --config ./webpack.dev.config.js && npx nodemon ./src/editor/main.mjs",
    "experiment-build": "rimraf -v ./dist && webpack",
    "cloudeditor-build": "rimraf -v ./public && webpack --config ./webpack.cloudeditor.config.js",
    "dev-build": "rimraf -v ./dist && webpack --config ./webpack.dev.config.js",
    "test": "jasmine --config=src/test/unit/jasmine.json",
    "clean": "rimraf -v ./dist",
    "clean-experiments": "node ./src/test/etc/clean_experiment_artifacts.mjs"
